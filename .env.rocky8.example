# App secrets
VITE_GOOGLE_MAPS_API_KEY=replace_me
RESEND_API_KEY=replace_me

# Shared webhook secret between Reportr API and n8n
N8N_AUTH_TOKEN=replace_with_long_random_token
N8N_AUTH_HEADER=x-reportr-key

# PostgreSQL (shared by n8n and optional app-side tooling)
POSTGRES_USER=reportr
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=reportr
N8N_DB_NAME=n8n

# n8n
N8N_ENCRYPTION_KEY=replace_with_32_plus_characters
N8N_HOST=your-vps-domain-or-ip
N8N_PROTOCOL=http
WEBHOOK_URL=http://your-vps-domain-or-ip:5678
GENERIC_TIMEZONE=Africa/Johannesburg

# MinIO
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=replace_with_strong_password
MINIO_BUCKET_ISSUES=reportr-issues
